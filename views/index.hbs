    <!-- INICIO CONTACTO -->

    <div class="container">
        <div class="row">
            <!-- preguntar porque no va junto con el class de abajo-->
            <div class="col-md-4 hero">
                <div id="contacto"></div>
                <form action="/" method="post">
                    <div class="form-group">
                        <label for="exampleFormControlInput1">Nombre</label>
                        <input type="nombre" class="form-control" id="exampleFormControlInput1" name="nombre">
                    </div>
                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Mail</label>
                        <input type="mail" class="form-control" id="exampleFormControlInput1" name="email">

                        </select>
                    </div>
                    <div class="form-group">
                        <label for="exampleFormControlSelect2">Teléfono</label>
                        <input type="telefono" class="form-control" id="exampleFormControlInput1" name="telefono">

                        </select>
                    </div>
                    <div class="form-group">
                        <label for="exampleFormControlTextarea1">Dejanos tu mensaje:</label>
                        <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="mensaje" ></textarea>
                    </div>
                    <button type="submit" class= "btn btn-primary"> Enviar</button>
                </form>
                {{message}}
            </div>
        </div>
    </div>
    <!-- FIN CONTACTO -->

